{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "homepage": "/wp-content/plugins/anycomment",
  "dependencies": {
    "ajv": "^6.5.3",
    "axios": "^0.18.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-dropzone": "^5.0.1",
    "react-inlinesvg": "^0.8.1",
    "react-scripts": "1.1.4",
    "react-string-replace": "^0.4.1",
    "react-timeago": "^4.1.9",
    "react-toastify": "^4.2.0",
    "react-tweet-embed": "^1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss -o src/css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/css --watch",
    "start": "rm -rf dist/* && parcel ./src/index.html",
    "build": "rm -rf dist/* && parcel build --no-cache ./src/index.html --public-url /wp-content/plugins/anycomment && mv dist/src*.css ../static/css/main.min.css && mv dist/src*.js ../static/js/main.min.js && mv dist/*.svg ../static/media/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1"
  }
}
